{
  "manifest_version": 2,
  "name": "Chrome Extention Sample",
  "version": "1.0.0",
  "description": "Chrome Extentionのサンプルです。",
  "icons": {
    "16": "images/icon/icon_16.png",
    "48": "images/icon/icon_48.png",
    "128": "images/icon/icon_128.png"
  },
  "content_scripts": [{
    "matches": ["http://www.yahoo.co.jp/*"],
    "css": [
      "css/style.css"
    ],
    "js": [
      "js/jquery-2.2.1.min.js",
      "js/script.js"
    ]
  }]
}